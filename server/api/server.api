// server.api
type (
	NodeWSReq {
		NodeId string `form:"id"`
		OS     string `form:"os"`
	}
	DeleteUserCache {
		UserName string `json:"user_name"`
	}
)

// @server (
//  middleware: JwtMiddleware // Enable jwt authentication
// )
service server-api {
	@handler nodeWS
	get /ws/node

	@handler deleteUserCache
	post /user/cache/delete (DeleteUserCache)
}

